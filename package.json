{
  "name": "@none/simply-inject-react-app",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "customize-cra": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@craco/types": "^7.1.0",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.36",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.1",
    "rollup-jest": "^3.1.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:co": "craco start",
    "build": "react-scripts build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "publish:lib": "npm run build:lib && cd ./src/_libs/injections && npm publish && cd ../..",
    "unpublish:lib": "cd ./src/_libs/injections && npm unpublish && cd ../..",
    "build:lib": "npm run cleardist && cd ./src/_libs/injections && npm run build && cd ../.. && npm run copy_readme",
    "build:lib:patch": "cd ./src/_libs/injections && npm --no-git-tag-version version patch && cd ../.. && build:lib",
    "cleardist": "rimraf rm ./src/_libs/injections/dist",
    "copy_readme": "copyfiles ./README.md ./src/_libs/injections/dist",
    "react-app-rewired": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^/(.*)$": "<rootDir>/src",
      "^injections(.*)$": "<rootDir>/src/_libs/injections/src$1"
    }
  }
}
